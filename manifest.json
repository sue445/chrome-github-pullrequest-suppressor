{
  "manifest_version": 2,

  "name": "upstream pullrequest suppressor for GitHub",
  "description": "Suppress PullRequest to upstream repository",
  "version": "0.0.1",

  "options_page": "options.html",

  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/compare/*"
      ],
      "js": [
        "lib/jquery-2.1.3.min.js",
        "src/app.js"
      ]
    }
  ]
}
